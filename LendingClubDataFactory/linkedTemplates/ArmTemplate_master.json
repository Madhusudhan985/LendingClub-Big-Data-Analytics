{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "LendingClubDataFactory"
		},
		"LinkedBlobService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedBlobService'"
		},
		"LinkedDatabricksService_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LinkedDatabricksService'"
		},
		"LinkedDatabricksService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0729-021527-5nwstw1w"
		},
		"ProcessedEventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/bac6087f-93aa-46cc-9ba7-2e863831218a/resourceGroups/Bigdata/providers/Microsoft.Storage/storageAccounts/lendingstoragesumit"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "LendingClubDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedBlobService_connectionString": {
						"value": "[parameters('LinkedBlobService_connectionString')]"
					},
					"LinkedDatabricksService_accessToken": {
						"value": "[parameters('LinkedDatabricksService_accessToken')]"
					},
					"LinkedDatabricksService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LinkedDatabricksService_properties_typeProperties_existingClusterId')]"
					},
					"ProcessedEventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('ProcessedEventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}